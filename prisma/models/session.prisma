model Session {
  id              String   @id @default(uuid())
  user_id         String
  token           String
  expires_at      DateTime
  ip_address      String?
  user_agent      String?
  impersonated_by String?
  created_at      DateTime @default(now())
  updated_at      DateTime @updatedAt

  user User @relation(fields: [user_id], references: [id])

  @@map("sessions")
}
